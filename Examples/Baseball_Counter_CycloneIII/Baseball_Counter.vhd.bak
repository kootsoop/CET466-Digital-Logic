						-- Output 1 = PIN J2
						-- Output 2 = PIN J3
						-- Output 3 = PIN H1
						-- skip Pin F2
						-- Output 4 = PIN E1
						-- Output 5 = PIN C1
						-- Output 6 = PIN C2				
						);